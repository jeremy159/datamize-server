{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            m.id as month_id,\n            m.month,\n            n.id as net_total_id,\n            n.type,\n            n.total,\n            n.percent_var,\n            n.balance_var\n        FROM balance_sheet_months AS m\n        JOIN balance_sheet_net_totals_months AS n ON m.id = n.month_id\n        JOIN balance_sheet_years AS y ON y.id = m.year_id AND y.year = $1\n        WHERE m.month = $2;\n        ",
